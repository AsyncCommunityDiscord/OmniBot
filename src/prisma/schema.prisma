
// === Modèles de core/models/config.prisma ===
model GuildConfiguration {
  guildId String @id
  data    Json
}

// === Modèles de core/models/modules.prisma ===
model ModuleActivation {
  moduleId String
  guildId  String

  activated        Boolean
  activatedVersion String  @default("")

  @@id([moduleId, guildId])
}
